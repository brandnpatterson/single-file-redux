{
  "name": "single-file-redux",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "react-scripts build && npm run create-cname",
    "create-cname": "echo 'single-file-redux.surge.sh' > build/CNAME",
    "deploy": "npm run build && surge build",
    "eject": "react-scripts eject",
    "start": "react-scripts start",
    "test": "react-scripts test --env=jsdom"
  },
  "dependencies": {
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-scripts": "1.1.4",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0"
  }
}
